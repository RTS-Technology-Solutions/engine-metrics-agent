# Firebase Configuration
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_API_KEY=your-api-key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your-project.appspot.com

# OpenAI Configuration (for LLM processing)
OPENAI_API_KEY=your-openai-api-key

# Google Cloud Configuration
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1

# Vertex AI Configuration
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL=text-bison@001

# Application Configuration
NODE_ENV=development
PORT=3000
API_BASE_URL=http://localhost:3000

# Database Configuration
FIRESTORE_EMULATOR_HOST=localhost:8080
FIREBASE_AUTH_EMULATOR_HOST=localhost:9099

# Storage Configuration
STORAGE_BUCKET_RAW=your-project-raw-data
STORAGE_BUCKET_PROCESSED=your-project-processed-data
STORAGE_BUCKET_EXPORTS=your-project-exports

# AI Processing Configuration
EMBEDDING_MODEL=text-embedding-004
LLM_MODEL=gemini-pro
MAX_TOKENS=4096
TEMPERATURE=0.7

# Monitoring and Logging
LOG_LEVEL=info
ENABLE_ANALYTICS=true

# Security Configuration
CORS_ORIGIN=http://localhost:3000
JWT_SECRET=your-jwt-secret-key

# Data Processing Configuration
BATCH_SIZE=100
MAX_FILE_SIZE=10MB
SUPPORTED_FORMATS=pgn,json,md,csv